<svg xmlns="http://www.w3.org/2000/svg" width="800" height="600" viewBox="0 0 800 600">
  <rect width="800" height="600" fill="#f0f0f0"/>
  <text x="400" y="100" font-family="Arial" font-size="32" text-anchor="middle" fill="#333">Sports Complex Image</text>
  <!-- Sports field -->
  <rect x="100" y="150" width="600" height="350" fill="#7cba6b" stroke="#5a8c4b" stroke-width="2"/>
  <!-- Running track -->
  <ellipse cx="400" cy="325" rx="250" ry="150" fill="none" stroke="#d35f5f" stroke-width="10"/>
  <!-- Basketball court -->
  <rect x="250" y="250" width="300" height="150" fill="#c19a6b" stroke="#99775a" stroke-width="2"/>
  <circle cx="400" cy="325" r="30" fill="none" stroke="#fff" stroke-width="2"/>
  <!-- Goal posts -->
  <rect x="150" y="300" width="10" height="50" fill="#fff"/>
  <rect x="640" y="300" width="10" height="50" fill="#fff"/>
</svg>